{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,
    "prettier.printWidth": 150,
    // "prettier.htmlWhitespaceSensitivity": "ignore",
    "prettier.trailingComma": "es5",
    "prettier.semi": true,
    // "liveServer.settings.useLocalIp": true,
    // "liveServer.settings.port": 5600,
    "liveSassCompile.settings.formats": [
        {
            "format": "compressed",
            "extensionName": ".css",
            "savePath": "/dist/css"
        }
    ],
    "liveServer.settings.port": 5501,
    "liveServer.settings.root": "/dist",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#cb6868",
        "activityBar.background": "#cb6868",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#215f21",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#cb6868",
        "statusBar.background": "#be4242",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#cb6868",
        "statusBarItem.remoteBackground": "#be4242",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#be4242",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#be424299",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#be4242"
}
